{
  "manifest_version": 3,
  "name": "scraper for the E-learning 3.0",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://lms3.ntpu.edu.tw/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content/jszip.min.js", "content/FileSaver.min.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["mime-db.json"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
